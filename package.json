    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "~9.1.1",
